{
  "name": "social-media-panel",
  "version": "0.1.0",
  "private": true,
  "prettier": "@Bitforge-LLC/prettier",
  "resolutions": {
    "@auth/core": "0.41.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "rm -rf src/lib/db/zod prisma zod && bunx prisma generate --schema=./src/lib/db/schema.prisma && next build --turbopack",
    "start": "next start",
    "lint": "eslint . --fix --cache --cache-location .next/cache/eslint",
    "post-edit": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json}'; eslint src --fix; tsc --noEmit",
    "vercel:build": "vercel build",
    "db:generate": "rm -rf src/lib/db/zod prisma zod && bunx prisma generate --schema=./src/lib/db/schema.prisma",
    "db:reset": "rm -rf src/lib/db/migrations && bunx prisma migrate dev --name init --schema=./src/lib/db/schema.prisma",
    "db:migrate": "bunx prisma migrate dev --schema=./src/lib/db/schema.prisma",
    "db:push": "bunx prisma db push --schema=./src/lib/db/schema.prisma",
    "db:view": "bunx prisma studio --schema=./src/lib/db/schema.prisma",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@Bitforge-LLC/crypto": "^0.0.32",
    "@Bitforge-LLC/devtools": "^0.1.35",
    "@Bitforge-LLC/email": "^0.1.6",
    "@Bitforge-LLC/ui": "^0.2.28",
    "@auth/prisma-adapter": "2.11.0",
    "@aws-sdk/client-s3": "^3.910.0",
    "@aws-sdk/s3-request-presigner": "^3.910.0",
    "@neondatabase/serverless": "^1.0.2",
    "@prisma/adapter-neon": "^6.17.1",
    "@prisma/client": "^6.17.1",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.0.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-query": "^5.90.3",
    "@trpc/client": "^11.6.0",
    "@trpc/next": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@trpc/tanstack-react-query": "^11.6.0",
    "axios": "^1.12.2",
    "framer-motion": "^12.24.10",
    "next": "^16.1.1",
    "next-auth": "^5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "superjson": "^2.2.2",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@Bitforge-LLC/eslint": "^0.0.15",
    "@Bitforge-LLC/prettier": "^0.0.3",
    "@Bitforge-LLC/testing": "^0.0.7",
    "@Bitforge-LLC/tsconfig": "^0.0.3",
    "@next/eslint-plugin-next": "16",
    "@playwright/test": "^1.48.2",
    "@tailwindcss/postcss": "^4.1.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^24.7.2",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^4.0.18",
    "eslint": "^9.37.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.6",
    "postcss-for": "^2.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-prisma": "^5.0.0",
    "prisma": "^6.19.0",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vercel": "^48.9.0",
    "vitest": "^4.0.18",
    "zod-prisma-types": "^3.3.5"
  }
}
